```swift
//=================================================
//My original attempt below: INCORRECT CODE

for i in 1...100 {
    if i.isMultiple(of: 3) && i.isMultiple(of: 5) {
        print("FizzBuzz")
    }
    if i.isMultiple(of: 3) {
        print("Fizz")
    }
    if i.isMultiple(of: 5) {
        print("Buzz")
    } else {
        print(i)
    }
}


//Correct code provided with the help of ChatGPT

for number in 1...100 {
    if number.isMultiple(of: 3) && number.isMultiple(of: 5) {
        print("FizzBuzz")
    } else if number.isMultiple(of: 3) {
        print("Fizz")
    } else if number.isMultiple(of: 5) {
        print("Buzz")
    } else {
        print(number)
    }
}







